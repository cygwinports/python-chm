  /usr/lib/python2.4/site-packages/chm/chm.pyo
  /usr/lib/python2.4/site-packages/chm/chmlib.py
  /usr/lib/python2.4/site-packages/chm/chmlib.pyc
  /usr/lib/python2.4/site-packages/chm/chmlib.pyo
  /usr/lib/python2.4/site-packages/chm/extra.dll
